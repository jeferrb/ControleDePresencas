        android:icon="@drawable/icon_app"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ActivityLogin"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityPrincipal"
            android:label="@string/title_activity_activity_principal"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ActivityAula"
            android:label="@string/title_activity_activity_aula"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ActivityAlterarPresencas"
            android:name=".ActivityProfConsAlunoTurma"
            android:label="@string/title_activity_activity_alterar_presencas"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ActivityRelatorioFinalAula"
            android:label="@string/title_activity_relatorio_final_aula"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
